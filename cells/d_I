PURGE a;
EDIT a;
WIN 0,0 10,10
ADD R1 0,0 10,10;
SAVE;
EXIT;

PURGE b;
EDIT b;
WIN -90,0 10,50
ADD a 0,0;
ADD a 60,50;
ADD R3 -90,50 -60,30;
SAVE;
EXIT;

PURGE c;
EDIT c;
WIN -120,-90 10,50
ADD a 0,0;
ADD b -100,-70;
ADD R3 -120,30 -80,-20;
ADD R2 -30,-60 0,-90;
SAVE;
EXIT;

PURGE d;
EDIT d;
WIN -120,-90 10,50
ADD c -140,-60;
SAVE;
EXIT;

